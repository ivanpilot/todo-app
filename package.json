{
    "name": "todos",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "check-types": "tsc",
        "lint": "next lint --ignore-path .gitignore",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx)\"",
        "validate": "npm run check-types && npm run format && npm run lint && npm run build",
        "test": "jest --watch"
    },
    "dependencies": {
        "next": "12.1.6",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "styled-components": "^5.3.5"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@types/node": "17.0.39",
        "@types/react": "18.0.11",
        "@types/react-dom": "18.0.5",
        "@types/styled-components": "^5.1.25",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "eslint": "8.17.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^26.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "lint-staged": "^13.0.0",
        "prettier": "^2.6.2",
        "typescript": "4.7.3"
    }
}
