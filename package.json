{
    "name": "todos",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "dev:app": "docker-compose down --remove-orphans && docker-compose -f ./docker-compose.yml up --build",
        "check-types": "tsc",
        "lint": "next lint --ignore-path .gitignore",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx)\"",
        "validate": "npm run check-types && npm run format && npm run lint && npm run build",
        "test": "jest --watch"
    },
    "prisma": {
        "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts"
    },
    "dependencies": {
        "@apollo/client": "^3.6.6",
        "@prisma/client": "^3.14.0",
        "apollo-server-micro": "^3.8.2",
        "graphql": "^16.5.0",
        "micro-cors": "^0.1.1",
        "next": "12.1.6",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "styled-components": "^5.3.5",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/user-event": "^14.2.0",
        "@types/micro-cors": "^0.1.2",
        "@types/node": "17.0.39",
        "@types/react": "18.0.11",
        "@types/react-dom": "18.0.5",
        "@types/styled-components": "^5.1.25",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "eslint": "8.17.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jest": "^26.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "lint-staged": "^13.0.0",
        "prettier": "^2.6.2",
        "prisma": "^3.14.0",
        "ts-node": "^10.8.1",
        "typescript": "4.7.3"
    }
}
